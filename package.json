{
  "name": "electron-auto-update-demo",
  "version": "1.0.0",
  "description": "A Electron auto update demo",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "app:pack": "electron-builder --dir",
    "app:dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.electron-auto-update-demo",
    "mac": {
      "category": "electron-auto-update-demo"
    }
  },
