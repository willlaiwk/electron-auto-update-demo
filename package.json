  "keywords": [
    "Electron",
    "Auto Update",
    "tutorial",
    "demo"
  ],
  "author": "Will Lai",
  "license": "MIT",
  "postinstall": "electron-builder install-app-deps",
  "devDependencies": {
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3"
  },
  "dependencies": {
    "electron-log": "^4.4.7",
    "electron-updater": "^5.0.1"
  }
}
